model order {
  misc {
    type ShippingAddress struct {
      Line1 string `json:"line1"`
      City string `json:"city"`
    }
  }

  fields {
    order_id() string
    total_amount() float32
    shipping_address() string
  }

  gens {
    func order_id() {
      return UUID()
    }

    func total_amount() {
      return FloatBetween(50, 500)
    }

    func shipping_address() {
      addr := &ShippingAddress{Line1: Sentence(3), City: Sentence(1)}
      return ToJSON(addr)
    }
  }
}
