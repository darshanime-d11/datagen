model orders {
  metadata {
    tags: {
        "service": "delivery",
        "team": "Slytherin"
    }
  }

  fields {
    order_id() string
    rider_id() int
    item() string
  }

  gens {
    func order_id() {
        return UUID()
    }

    func rider_id() {
        return IntBetween(1, 500)
    }

    func item() {
        return RandomFrom("sushi", "pasta", "pizza")
    }
  }
}