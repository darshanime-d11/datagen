model users {
  metadata {
    tags: {
      "service":  "user",
      "team":  "Slytherin"
    }
  }

  fields {
    id() int
    name() string
    email() string
  }

  gens {
    func id() {
      return iter + 1
    }

    func name() {
      return Name()
    }

    func email() {
      return Email()
    }
  }
}
